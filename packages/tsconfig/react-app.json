{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React App",
  "extends": "./base.json",
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "noErrorTruncation": true,
    "isolatedModules": false,
    "jsx": "react-jsx",
    "lib": [
      "ESNext",
      "dom"
    ],
    "module": "ESNext",
    "sourceMap": true,
    "target": "ES5"
  }
}
